
public class main {

	public static void main(String[] args) {
		
		
		System.out.println(max(12, 55, 45));
	}
	
	public static int max(int a, int b, int c) {
		
		int maior;
		
		if (a > b && a > c) {
			maior = a;
		}
		else if (b > c) {
			maior = b;
		}
		else if (a == b ||a == c || b == c) {
			maior = a;
		}
		else {
			maior = c;
		}
		return maior;
	}
}
