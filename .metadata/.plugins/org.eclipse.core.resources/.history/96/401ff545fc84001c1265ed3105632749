package intities;

public class registor {
	
	private String name;
	private String email;
	private int room;
	
	public registor(int studentroom, String name, String email) {
		this.room = studentroom;
		this.name = name;
		this.email = email;	
	}
	
	public registor() {
		
	}
	
	public int getRoom () {
		return room;
	}
	
	public void setRoom (int room) {
		this.room = room;
	}

	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public String getEmail() {
		return email;
	}
	
	public void setEmail(String email) {
		this.email = email;
	}
	
	public void printMod() {
		System.out.println("Busy rooms:");
		System.out.println(room + ": " + name + ", " + email);
	}

}
