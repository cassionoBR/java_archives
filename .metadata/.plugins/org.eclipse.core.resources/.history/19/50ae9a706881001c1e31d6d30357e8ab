import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int num, cont;
		
		System.out.print("Enter a number: ");
		num = sc.nextInt();
		
		cont = 0;
		
		for (int i = 0; i < num; i++) {
			
				if (i % 2 == 1) {
					System.out.println(i);
				}
		}
		
		sc.close();
	}	
}
